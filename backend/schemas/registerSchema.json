{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": ["username", "password", "email"],
  "properties": {
    "username": {
      "type": "string",
      "default": "",
      "title": "The username Schema",
      "examples": ["blob"]
    },
    "password": {
      "type": "string",
      "default": "",
      "title": "The password Schema",
      "examples": ["bleep"]
    },
    "first_name": {
      "type": "string",
      "default": "",
      "title": "The first_name Schema",
      "examples": ["corey"]
    },
    "last_name": {
      "type": "string",
      "default": "",
      "title": "The last_name Schema",
      "examples": ["jimenez"]
    },
    "email": {
      "type": "string",
      "default": "",
      "format": "email",
      "title": "The email Schema",
      "examples": ["email@gmail"]
    }
  },
  "examples": [
    {
      "username": "blob",
      "password": "bleep",
      "first_name": "corey",
      "last_name": "jimenez",
      "email": "email@gmail"
    }
  ]
}
